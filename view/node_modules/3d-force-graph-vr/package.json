    "types": "./dist/3d-force-graph-vr.d.ts",
    "umd": "./dist/3d-force-graph-vr.min.js",
    "default": "./dist/3d-force-graph-vr.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/3d-force-graph-vr.git"
  },
  "homepage": "https://github.com/vasturiano/3d-force-graph-vr",
  "keywords": [
    "3d",
    "force",
    "graph",
    "vr",
    "aframe",
    "d3"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "bugs": {
