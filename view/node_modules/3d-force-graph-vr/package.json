{
  "name": "3d-force-graph-vr",
  "version": "2.2.5",
  "description": "UI component for a 3D force-directed graph in VR",
  "license": "MIT",
  "type": "module",
  "unpkg": "dist/3d-force-graph-vr.min.js",
  "main": "dist/3d-force-graph-vr.mjs",
